{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",  
  "scripts": {
    "dev:react": "vite",
    "dev:ts": "tsc --watch",
    "dev:electron": "electron ./dist-electron/main.js",
    "dev": "concurrently \"npm:dev:react\" \"npm:dev:ts\" \"npm:dev:electron\"",
    "build:electron": "tsc",
    "build:react": "vite build",
    "build": "npm run build:electron && npm run build:react",
    "dist": "cross-env NODE_ENV=production npm run build && electron-builder",
    "dist:win": "cross-env NODE_ENV=production npm run build && electron-builder --win",
    "dist:mac": "cross-env NODE_ENV=production npm run build && electron-builder --mac",
    "dist:linux": "cross-env NODE_ENV=production npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@nestjs/platform-socket.io": "^11.1.3",
    "@nestjs/websockets": "^11.1.3",
    "axios": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.6.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ssh2": "^1.16.0",
    "uuid": "^11.1.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "electron": "^37.2.3",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "headlessui": "^0.0.0",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.3",
    "wait-on": "^8.0.3"
  }
}
